{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es5",
    "allowJs": true,
    "sourceMap": true,
    "alwaysStrict": true,
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "include": [
    "src",
    "bin/root/src/WebApi",
    "bin/root/src/Http"
  ]
}
